pipeline{
    agent any

    stages{
        stage('Build') {
            steps {
                echo "building"
            }
        }
       
        stage('Test') {
            steps {
                echo "this is the mohan test"
            }
        }

        stage('deploy') {
            steps {
                echo "deploying the succesful production to the test"
            }
        }
    }
    
}